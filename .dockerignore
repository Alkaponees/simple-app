# Ignore node_modules, dependencies installed in container
node_modules
npm-debug.log
yarn-error.log
pnpm-debug.log

# Ignore environment files (they are injected at runtime from Secrets Manager)
.env
.env_*
*.env

# Ignore certificates (they will be mounted / generated at runtime)
certs/
*.crt
*.key

# Ignore git and CI/CD configs
.git
.gitignore
.dockerignore
.github/

# Ignore OS / editor files
.DS_Store
*.swp
*.swo
*.bak
Thumbs.db

# Ignore Terraform, Docker Compose local configs
*.tfstate
*.tfstate.*
.terraform/
docker-compose.override.yml
